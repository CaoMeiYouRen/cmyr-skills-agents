{
  "name": "cmyr-skills-agents",
  "version": "0.1.0",
  "description": "草梅友仁的个性化 AI 技能库和 AI 智能体库",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "release": "semantic-release",
    "lint": "cross-env NODE_ENV=production eslint . --fix",
    "lint:md": "lint-md *.md .github/**/*.md --fix",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsx scripts/cli.ts",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsdown": "cross-env NODE_ENV=development tsdown --watch",
    "start": "tsx scripts/cli.ts",
    "start:tsx": "cross-env NODE_ENV=production tsx src/index.ts",
    "commit": "cz",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@clack/prompts": "^1.0.0",
    "@lint-md/cli": "^2.0.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^25.1.0",
    "commitizen": "^4.3.1",
    "commitlint": "^20.4.0",
    "commitlint-config-cmyr": "^1.0.0",
    "cross-env": "^10.0.0",
    "cz-conventional-changelog-cmyr": "^2.0.0",
    "debug": "^4.4.3",
    "eslint": "^9.34.0",
    "eslint-config-cmyr": "^2.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.3",
    "semantic-release-cmyr-config": "^1.0.1",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.20.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "config": {},
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "ai",
    "skills",
    "agents",
    "typescript"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/cmyr-skills-agents#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/cmyr-skills-agents.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/cmyr-skills-agents/issues"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "semver": "^7.7.3",
      "@typescript-eslint/parser": "^8.54.0",
      "@typescript-eslint/eslint-plugin": "^8.54.0",
      "@typescript-eslint/utils": "^8.54.0",
      "@typescript-eslint/type-utils": "^8.54.0",
      "@typescript-eslint/scope-manager": "^8.54.0",
      "@typescript-eslint/visitor-keys": "^8.54.0",
      "@typescript-eslint/typescript-estree": "^8.54.0",
      "compare-func": "^2.0.0",
      "katex": "^0.16.21",
      "esbuild": "^0.25.0",
      "@octokit/plugin-paginate-rest": "^11.4.1",
      "@octokit/request": "^9.2.1",
      "@octokit/request-error": "^5.1.1",
      "brace-expansion": "^2.0.2",
      "tmp": "^0.2.4",
      "js-yaml": "^4.1.1"
    }
  },
  "type": "module",
  "changelog": {
    "language": "zh"
  }
}